    <dc:title>Irish technology at forefront of Obama's stimulus plan</dc:title>
    <sioc:has_creator rdf:nodeID="A155"/>
    <dcterms:created>Irish technology at forefront of Obama's stimulus plan</dcterms:created>
    <sioc:content></sioc:content>
