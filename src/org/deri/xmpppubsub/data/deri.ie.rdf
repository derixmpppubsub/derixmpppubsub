    <bibtex:hasYear>2007</bibtex:hasYear>
    <bibtex:hasVolume>243</bibtex:hasVolume>
    <bibtex:hasNumber></bibtex:hasNumber>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.deri.ie/publications/monographs/proceedings/#pp_36">
